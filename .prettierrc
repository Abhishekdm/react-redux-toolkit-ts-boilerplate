{
  "singleQuote": false,
  "trailingComma": "all",
  "tabWidth": 2,
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "jsxSingleQuote": false,
  "printWidth": 100,
  "proseWrap": "always",
  "quoteProps": "as-needed",
  "semi": true,
  "useTabs": false,
  "overrides": [
    {
      "files": [".eslintrc"],
      "options": {
        "trailingComma": "none"
      }
    }
  ]
}
